{
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.fixAll.eslint": "explicit",
		"eslint.autoFixOnSave": "explicit",
		"source.organizeImports": "never",
		"source.fixAll.stylelint": "explicit"
	},
	"eslint.validate": [
		"javascript",
		"typescript"
	],
	"cSpell.words": [
		"defaultify"
	],
	"files.eol": "\n"
}